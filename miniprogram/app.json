    "pages/rank/rank",
    "pages/shop/shop",
    "pages/user/my/my",
    "pages/user/profile/profile",
    "pages/user/creditHistory/creditHistory",
    "pages/admin/settingList/settingList",
    "pages/admin/activityDetail/activityDetail",
    "pages/admin/activityList/activityList",
    "pages/admin/userList/userList",
    "pages/admin/userDetail/userDetail",
    "pages/admin/eventList/eventList"
  ],
  "window": {
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#000008",
    "navigationBarTitleText": "EABC",
    "navigationBarTextStyle": "white",
    "navigationStyle": "custom"
  },
  "tabBar": {
    "backgroundColor": "#FFFFFF",
    "borderStyle": "black",
    "color": "#A7A7A7",
    "selectedColor": "#2D8DB8",
    "list": [
      {
        "pagePath": "pages/home/home",
        "iconPath": "static/icons/tabbar/badminton-7A7A7A.png",
        "selectedIconPath": "static/icons/tabbar/badminton-90CAF9.png",
        "text": "活动"
      },
      {
        "pagePath": "pages/rank/rank",
        "iconPath": "static/icons/tabbar/rank-7A7A7A.png",
        "selectedIconPath": "static/icons/tabbar/rank-90CAF9.png",
        "text": "排名"
      },
      {
        "pagePath": "pages/user/my/my",
        "iconPath": "static/icons/tabbar/badge-7A7A7A.png",
        "selectedIconPath": "static/icons/tabbar/badge-90CAF9.png",
        "text": "我的"
      }
    ]
  },
  "style": "v2",
  "renderer": "skyline",
  "rendererOptions": {
    "skyline": {
      "disableABTest": true,
      "defaultDisplayBlock": true
    }
  },
  "componentFramework": "glass-easel",
  "sitemapLocation": "sitemap.json",
  "useExtendedLib": {
    "weui": true
  },
  "lazyCodeLoading": "requiredComponents",
  "resolveAlias": {
    "@Language/*": "languages/*",
    "@API/*": "apis/*",
    "@Lib/*": "utils/*",
    "@Model/*": "models/*"
  }
}