module.exports = {
  includesUser: function (attendees, memberId) {
    var findMatch = false;
    for (var i = 0; i < attendees.length; i++) {
      var user = attendees[i];
      if (user.memberId === memberId) {
        findMatch = true;
      }
    }
    return findMatch;
  },
  includes: function (array, element) {
    if (!array) return false;
    var findMatch = false;
    for (var i = 0; i < array.length; i++) {
      var item = array[i];
      if (item === element) {
        findMatch = true;
      }
    }
    return findMatch;
  },
  filter: function(arr, predicate) {
    var result = [];
    for (var i = 0; i < arr.length; ++i) {
      if (predicate(arr[i], i, arr)) {
        result.push(arr[i]);
      }
    }
    return result;
  },


}

